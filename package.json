{
  "name": "prettier-plugin-md-nocjsp",
  "version": "1.0.2",
  "description": "Prettier Markdown plugin to prevent inserting spaces between Chinese or Japanese & latin latters",
  "main": "dist/main.js",
  "repository": "https://github.com/tats-u/prettier-plugin-md-nocjsp",
  "author": "Tatsunori Uchino",
  "license": "MIT",
  "keywords": [
    "prettier",
    "plugin",
    "markdown",
    "japanese"
  ],
  "files": [
    "dist/main.js"
  ],
  "scripts": {
    "test": "prettier --plugin . --parser markdown-nocjsp ./assets/*.md -f",
    "build": "rollup -c"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "cjk-regex": "2.0.0",
    "escape-string-regexp": "4.0.0",
    "linguist-languages": "7.12.1",
    "outdent": "0.7.1",
    "prettier": "^2.2.1",
    "remark-footnotes": "2.0.0",
    "remark-math": "3.0.1",
    "remark-parse": "8.0.3",
    "rollup": "^2.41.2",
    "rollup-plugin-terser": "^7.0.2",
    "semver": "7.3.2",
    "string-width": "4.2.0",
    "unified": "9.2.0"
  }
}
